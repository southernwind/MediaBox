<ResourceDictionary 
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="clr-namespace:SandBeige.MediaBox.Composition.Enum">
	<ObjectDataProvider x:Key="DisplayMode" MethodName="GetValues" ObjectType="{x:Type local:DisplayMode}">
		<ObjectDataProvider.MethodParameters>
			<x:Type TypeName="local:DisplayMode"/>
		</ObjectDataProvider.MethodParameters>
	</ObjectDataProvider>
	<local:DisplayModeToStringConverter x:Key="DisplayModeToStringConverter"/>
</ResourceDictionary>