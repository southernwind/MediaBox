<UserControl
	x:Class="SandBeige.MediaBox.Views.Media.Library.Media"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:if="clr-namespace:SandBeige.MediaBox.Composition.Interfaces;assembly=MediaBox.Composition"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	d:DataContext="{d:DesignInstance if:IMediaFileViewModel}"
	mc:Ignorable="d">
	<Image Source="{Binding Thumbnail.ImageSource, IsAsync=True}" />
</UserControl>
