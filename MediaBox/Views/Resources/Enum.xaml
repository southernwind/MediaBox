<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:enum="clr-namespace:SandBeige.MediaBox.Composition.Enum;assembly=MediaBox.Composition">
	<ObjectDataProvider
		x:Key="DisplayMode"
		MethodName="GetValues"
		ObjectType="{x:Type enum:DisplayMode}">
		<ObjectDataProvider.MethodParameters>
			<x:Type TypeName="enum:DisplayMode" />
		</ObjectDataProvider.MethodParameters>
	</ObjectDataProvider>
	<enum:DisplayModeToStringConverter x:Key="DisplayModeToStringConverter" />
</ResourceDictionary>