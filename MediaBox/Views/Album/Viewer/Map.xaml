<UserControl
	x:Class="SandBeige.MediaBox.Views.Album.Viewer.Map"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:map="clr-namespace:SandBeige.MediaBox.Views.Map"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:viewerVm="clr-namespace:SandBeige.MediaBox.ViewModels.Album.Viewer"
	d:DataContext="{d:DesignInstance viewerVm:MapViewerViewModel}"
	d:DesignHeight="450"
	d:DesignWidth="800"
	mc:Ignorable="d">
	<DockPanel>
		<map:MapControl x:Name="MapControl" DataContext="{Binding Map.Value}" />
	</DockPanel>
</UserControl>
