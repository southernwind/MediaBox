<UserControl x:Class="SandBeige.MediaBox.Views.SubWindows.OptionWindow.Pages.GeneralSettings"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:local="clr-namespace:SandBeige.MediaBox.Views.SubWindows.OptionWindow.Pages"
			 xmlns:vm="clr-namespace:SandBeige.MediaBox.ViewModels.SubWindows.OptionWindow.Pages"
			 mc:Ignorable="d" 
			 d:DesignHeight="450" d:DesignWidth="800"
			 d:DataContext="{d:DesignInstance vm:GeneralSettingsViewModel}">
	<StackPanel>
		<GroupBox>
			<StackPanel>
				<TextBox Text="{Binding BingMapApiKey.Value}"/>
			</StackPanel>
		</GroupBox>
	</StackPanel>
</UserControl>
