<UserControl
	x:Class="SandBeige.MediaBox.StyleChecker.Views.Pages.GroupBox"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:pageVm="clr-namespace:SandBeige.MediaBox.StyleChecker.ViewModels.Pages"
	xmlns:sc="clr-namespace:SandBeige.MediaBox.StyleChecker.Controls;assembly=MediaBox.StyleChecker.Controls"
	d:DataContext="{d:DesignInstance pageVm:GroupBoxViewModel}"
	d:DesignHeight="450"
	d:DesignWidth="800"
	mc:Ignorable="d">
	<StackPanel>
		<WrapPanel Orientation="Horizontal">
			<sc:StyleViewer Header="GroupBox">
				<GroupBox Content="コンテンツ" Header="ヘッダー" />
			</sc:StyleViewer>
		</WrapPanel>
	</StackPanel>
</UserControl>
